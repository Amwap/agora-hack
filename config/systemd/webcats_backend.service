[Unit]
Description=webcats develop

[Service]
User=webcats
Restart=always
RestartSec=5
Nice=1
SuccessExitStatus=0 1
WorkingDirectory=/home/webcats/webcats/
ExecStart=/home/webcats/webcats/venv/bin/uwsgi --ini config/uwsgi/uwsgi.ini
ExecStop=/home/webcats/webcats/venv/bin/uwsgi --stop config/uwsgi/webcats.pid

[Install]
WantedBy=multi-user.target
